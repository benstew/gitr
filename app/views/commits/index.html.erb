<p id="notice"><%= notice %></p>

<h1>Listing Commits</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commits.each do |commit| %>
      <tr>
        <td><%= link_to commit.message, commit.url %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
