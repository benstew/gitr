<script src="dashboard/dashboard.js"></script>
<script id="entry-template" type="text/x-handlebars-template">
	<h1>Dashboard</h1>
	<h2>Your Projects</h2>
	<ul>
	
	{{#each projects as |project|}}
		<li class="project-li" data-project-id="{{project.id}}">
		 <a href="#">{{ project.name }}</a>
		 </li>
	{{/each}}
	</ul>
</script>
<script id= "project-details-template" type="text/x-handlebars-template">
  <p>
  {{project.name}}
  </p>
   <p>
  {{project.url}}
  </p>
   <p>
  {{project.admin.user.name}}
  </p>
   <p>
    {{#each project.collaborators as |collaborator|}}
      {{collaborator.user.name}}
    {{/each}}
  </p>
   <p>
    {{#each project.milestones as |milestone|}}
      {{milestone.title}}
    {{/each}}
  </p>

</script>

<% if logged_in? %>
<div class="container">
	<div class="row">
		<div class="col-xs-3">
   
			<div id="projects-board"></div>
      
		</div>
		<div class= "col-xs-9">
      <div id="project-details"></div>

		</div>
	</div>
</div>






<% end %>
